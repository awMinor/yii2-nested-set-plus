{
  "name": "kgladkiy/yii2-nested-set-plus",
  "version": "0.2",
  "description": "This extension allows you to get functional for nested set trees and widget for show and manipulate nested data in view.",
  "keywords": [
    "yii2",
    "extension",
    "yii2 behavior",
    "yii2 widget",
    "nested set"
  ],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Kirill Gladkiy",
      "email": "kirill.gladkiy@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "yiisoft/yii2": "*"
  },
  "autoload": {
    "psr-4": {
      "kgladkiy\\behaviors\\": "behaviors",
      "kgladkiy\\widgets\\": "widgets"
    }
  }
}
